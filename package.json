{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-from-new": "git submodule update --init --recursive --remote --force && hexo generate",
    "build": "git submodule update --remote --force && hexo generate",
    "clean": "git submodule update --remote --force && hexo clean",
    "deploy": "git submodule update --remote --force && hexo deploy",
    "server": "git submodule update --remote --force && hexo server"
  },
  "hexo": {
    "version": "7.0.0"
  },
  "dependencies": {
    "hexo": "^7.1.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-inferno": "^0.1.3",
    "hexo-renderer-marked": "^6.3.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-icarus": "^6.1.0"
  }
}
